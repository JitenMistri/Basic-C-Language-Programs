#include <conio.h>
#include <stdio.h>
main()
{
	int n,i=1,count=0,s=0;
	printf("enter the number upto which the sum is required: ");
	scanf("%d",&n);
	while(i<=n)
	{
		if(i%2!=0) // odd numbers btwn n
			count=count+i;
		s=s+2*i-1; // n odd numbers
		i++;
	}
	printf("the sum is: %d",count);
	printf("the sum is: %d",s);
}
